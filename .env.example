# Copie para .env (este arquivo NAO deve ser commitado)
# cp .env.example .env

# Gemini (IA)
GEMINI_API_KEY=

# Modelo preferido (opcional). Se vazio, usa o default do backend.
GEMINI_MODEL=

# Modelo fallback (opcional). Usado quando quota do preferido estoura ou fail-safe.
GEMINI_FALLBACK_MODEL=

# Quota do modelo preferido por janela (opcional). Default: 3000
GEMINI_PREFERRED_MODEL_REQUEST_LIMIT=

GEMINI_TIMEOUT_MS=20000
